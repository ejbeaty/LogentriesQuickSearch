{
  "manifest_version": 2,
  "name": "Stratify Error Lookup",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "js/jquery-3.3.1.min.js" ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "options_page": "options/options.html",
  "permissions": [
    "storage"
  ]
}